<master src="/packages/xcms-ui/www/master">
<property name="title">@label;noquote@</property>
<property name="context">@context;noquote@</property>

<p>
<include src="folder-chunk" folder_id="@folder_id@" folder_name="" parent_id="@parent_id@">
</p>
<multiple name="folders">
<p>
<h4><a href="@folders.name@/">@folders.label@</a></h4>
</p>
</multiple>


